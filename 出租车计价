#include <stdio.h>  
int main()  
{  
    float s;
    int t;
    float price=0;
    
    scanf("%f %d",&s,&t);
    
    if(s<=3){
        price=10;
    }else if(3<s&&s<10){
        price=10+(s-3)*2;
    }else{
        price=10+7*2+(s-10)*3;
    }
    
    if(t<5){
        printf("%.0f",price);
    }else{
        price=price+t/5*2;
        printf("%.0f",price);
    }

    
    return 0;  
}
